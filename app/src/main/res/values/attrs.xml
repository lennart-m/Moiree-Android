<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HsbColorPicker">
        <attr name="selectedColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SeekPane">
        <attr name="maxX" format="integer"/>
        <attr name="posX" format="integer"/>
        <attr name="stepX" format="integer"/>
        <attr name="maxY" format="integer"/>
        <attr name="posY" format="integer"/>
        <attr name="stepY" format="integer"/>
        <attr name="thumbDrawable" format="reference"/>
    </declare-styleable>

    <attr name="state_input_active" format="boolean"/>

    <declare-styleable name="IntValueSetup">
        <attr name="minValue" format="integer"/>
        <attr name="maxValue" format="integer"/>
        <attr name="value" format="integer"/>
        <attr name="stepSize" format="integer"/>
        <attr name="name" format="string"/>
        <attr name="allowStepOutOfBounds" format="boolean"/>
        <attr name="textFormatter" format="reference"/>
    </declare-styleable>

    <declare-styleable name="Expandable">
        <attr name="expanded" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ExpandableView" parent="Expandable"/>

    <attr name="title" format="string"/>

    <declare-styleable name="ColorPreviewHeader">
        <attr name="title"/>
        <attr name="color" format="color"/>
    </declare-styleable>

    <declare-styleable name="ArrowExpandableHeader" parent="Expandable">
        <attr name="title"/>
    </declare-styleable>

    <declare-styleable name="DrawableProgressPreviewHeader">
        <attr name="title"/>
        <attr name="busy" format="boolean"/>
        <attr name="drawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ForceChildrenSquareViewGroup">
        <attr name="cropChildren" format="boolean"/>
    </declare-styleable>

</resources>
